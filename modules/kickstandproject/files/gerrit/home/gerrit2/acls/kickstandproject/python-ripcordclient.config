[access "refs/heads/*"]
        label-Approved = +0..+1 group ripcord-core
        label-Code-Review = -2..+2 group ripcord-core
[access "refs/tags/*"]
        create = group ripcord-core
        pushTag = group ripcord-core
[project]
        state = active
[receive]
        requireChangeId = true
[submit]
        mergeContent = true
